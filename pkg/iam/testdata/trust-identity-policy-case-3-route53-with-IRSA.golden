{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::751852626996:role/apie1-kiam-role"
      },
      "Action": "sts:AssumeRole"
    },
    {
      "Effect": "Allow",
      "Principal": {
        "Federated": "arn:aws:iam::751852626996:oidc-provider/d12qpcaph79a8w.cloudfront.net"
      },
      "Action": "sts:AssumeRoleWithWebIdentity",
      "Condition": {
        "StringEquals": {
          "d12qpcaph79a8w.cloudfront.net:sub": ["system:serviceaccount:kube-system:external-dns", "system:serviceaccount:kube-system:cert-manager-controller"]
        }
      }
    }
  ]
}
